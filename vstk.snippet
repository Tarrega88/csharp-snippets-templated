<CodeSnippet Format="1.0.0">
  <Header>
    <Title>vstk</Title>
    <Shortcut>vstk</Shortcut>
    <Description>Template for Stack</Description>
    <Author>Michael See</Author>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>type</ID>
        <ToolTip>Data type</ToolTip>
        <Default>T</Default>
      </Literal>
      <Literal>
        <ID>name</ID>
        <ToolTip>Variable name</ToolTip>
        <Default>placeholder</Default>
      </Literal>
    </Declarations>
    <Code Language="csharp">
      <![CDATA[
var $name$ = new Stack<$type$>();
      ]]>
    </Code>
  </Snippet>
</CodeSnippet>
